{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build Veh",
            "type": "shell",
            "command": "C:/msys64/mingw64/bin/g++.exe",
            "args": [
                "-g",
                "main.cpp",
                "Vehicle.cpp",
                "Car.cpp",
                "Truck.cpp",
